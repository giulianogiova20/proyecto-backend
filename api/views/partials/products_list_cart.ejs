<% if(products.length) { %> 
    <% products.map(product => { %> 
        <tr>
            <td><%= product.id %></td>
            <td><%= product.name %></td>
            <td><%= product.price %></td>
            <td><%= product.description %> </td>
            <td><%= product.stock %> </td>
            <td><img style="max-width: 80px" src="<%= product.photoURL%>"></img></td>
        </tr>
    <% }) %> 
    
<% } else { %> 
    <h1 class="text-danger">NO PRODUCTS</h1>
    <br>
    <h2><a type="button" href="/">Go to Store>></a></h2>
<% } %>