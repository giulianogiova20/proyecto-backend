<% if(products.length) { %> 
    <% products.map(product => { %> 
        <tr>
            <td><%= product.id %></td>
            <td><%= product.name %></td>
            <td><%= product.price %></td>
            <td><%= product.description %> </td>
            <td><%= product.stock %> </td>
            <td><img style="max-width: 100px" src="<%=product.photoURL%>"></img></td>
        </tr>
    <% }) %> 

<% } else { %> 
    <h1>NO PRODUCTS - Add a new product in Form</h1>
    <br>
    <a type="button" href="/">Go to form</a>
<% } %>